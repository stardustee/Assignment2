 

 
 <div class="row justify-content-md-center">
          <div class="col">

            <a href="/buisness-list/add" class="btn btn-primary btn-lg"><i class="fas fa-plus-circle"></i> Add New Contact</a>
            <hr>

            <table class="table table-striped table-bordered table-hover">
              <thead class="thead-dark">
                <tr>
                  <th scope="col">Name</th>
                  <th scope="col">Number</th>
                  <th scope="col">Email Address</th>
                  <th></th>
                  <th></th>
                </tr>
              </thead>
              <tbody>

                <% for(let count=0; count < Buisness.length; count++) { %>

                <tr>
                  <td><%= Buisness[count].name %></td>
                  <td><%= Buisness[count].number %></td>
                  <td><%= Buisness[count].email %></td>
                  <td>
                    <a href="/buisness-list/edit/<%= buisness[count]._id %>" class="btn btn-primary btn-sm">
                        <i class="fas fa-edit"></i> Update</a>
                  </td>
                  <td>
                      <a href="/buisness-list/delete/<%= buisness[count]._id %>" class="btn btn-danger btn-sm">
                          <i class="fas fa-trash-alt"></i> Delete</a>
                  </td>
                </tr>
                
              <% } %>

              </tbody>
            </table>

          </div>
        </div>